#: PlaceFrCog/placefrcog.gpr.py:30
msgid "Lokoj : Franca Oficiala Geografia Kodo"
msgstr "Lugares: código geográfico oficial francês"

#: PlaceFrCog/placefrcog.gpr.py:31
msgid "Gramplet por preni municipojn de la datumbazo Insee"
msgstr "Gramplet para tirar municípios do banco de dados INSEE"

#: PlaceFrCog/placefrcog.gpr.py:37
msgid "Fr COG"
msgstr "Frg"

#: PlaceFrCog/placefrcog.py:96
msgid "Enigu la INSEE-numeron de la komunumo: (aŭ plurajn nombrojn apartigitajn per spacoj)"
msgstr "Digite o número INSEE da comunidade: (ou vários números separados por espaços)"

#: PlaceFrCog/placefrcog.py:101
msgid "Ŝargi per kodo"
msgstr "Carregue com código"

#: PlaceFrCog/placefrcog.py:106
msgid "Ŝargi per kodo (anstataŭe)"
msgstr "Carregue com o código (em vez disso)"

#: PlaceFrCog/placefrcog.py:113
msgid "Enigu la nomon de la komunumo:"
msgstr "Digite o nome da comunidade:"

#: PlaceFrCog/placefrcog.py:118
msgid "Ŝarĝi per nomo"
msgstr "Carregar pelo nome"

#: PlaceFrCog/placefrcog.py:123
msgid "Ŝarĝi per nomo (anstataŭe)"
msgstr "Carregar pelo nome (em vez disso)"

#: PlaceFrCog/placefrcog.py:159 PlaceFrCog/placefrcog.py:258
#, python-format
msgid "Ĝisdatigo de loko kun INSEE-id %s"
msgstr "Atualização do lugar com ID ID %s"

#: PlaceFrCog/placefrcog.py:183 PlaceFrCog/placefrcog.py:247
msgid "Ekzistanta kodo"
msgstr "Código existente"

#: PlaceFrCog/placefrcog.py:183 PlaceFrCog/placefrcog.py:247
msgid "Ĉi tiu kodo jam ekzistas en la datumbazo."
msgstr "Este código já existe no banco de dados."

#: PlaceFrCog/placefrcog.py:193
#, python-format
msgid "Aldono de loko kun INSEE-id %s"
msgstr "Adicionando lugar com INSEE ID %s"

#: PlaceFrCog/placefrcog.py:222
msgid "Nekonato nomo"
msgstr "Nome desconhecido"

#: PlaceFrCog/placefrcog.py:222
msgid "Ĉi tiu nomo ne estis trovita en la INSEE-datumbazo: "
msgstr "Este nome não foi encontrado no banco de dados INSEE:"

#: PlaceFrCog/placefrcog.py:229
#, python-format
msgid "%d rezultoj"
msgstr "%d Resultados"

#: PlaceFrCog/placefrcog.py:229
#, python-format
msgid "Unua rezulto :\n\t%s ; kodo=%s. \n\n\t\tImporti ?\n"
msgstr ""
"Primeiro resultado:\n"
"\t%s ; código=%s. \n"
"\n"
"\t\tImportar ?\n"

#: PlaceFrCog/placefrcog.py:230
msgid "Ies, Importi"
msgstr "Ies, importação"

#: PlaceFrCog/placefrcog.py:230
msgid "Venonta"
msgstr "Próximo"

#: PlaceFrCog/placefrcog.py:254 PlaceFrCog/placefrcog.py:384
msgid "Nekonata kodo"
msgstr "Código desconhecido"

#: PlaceFrCog/placefrcog.py:254 PlaceFrCog/placefrcog.py:384
msgid "Ĉi tiu kodo ne estis trovita en la INSEE-datumbazo: "
msgstr "Este código não foi encontrado no banco de dados INSEE:"

#: PlaceFrCog/placefrcog.py:293
#, python-format
msgid "Aldono de loko %s"
msgstr "Adicionando %s"
